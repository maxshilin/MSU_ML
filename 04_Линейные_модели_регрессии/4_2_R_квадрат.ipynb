   "source": [
    "import numpy as np\n",
    "\n",
    "\n",
    "def r2(y_true: np.array, y_pred: np.array):\n",
    "    De = np.sum(np.power(y_true - y_pred, 2))\n",
    "    Dz = np.sum(np.power(y_true - y_true.mean(), 2))\n",
    "    return 1 - De / Dz"
   ]
  }
